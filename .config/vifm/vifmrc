" vim: filetype=vifm :

let $conf = $VIFM . '/settings'
source $conf
let $conf = $VIFM . '/filetypes'
source $conf
let $conf = $VIFM . '/commands'
source $conf
let $conf = $VIFM . '/maps'
source $conf

let $localConf = $VIFM . '/vifmrc-local'

if filereadable($localConf)
    source $localConf
endif

