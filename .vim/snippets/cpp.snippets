snippet ifor
    for (int ${1:i} = 0; ${1:i}<${2:max}; ${1:i}++) {
        ${0}
    }

snippet dfor
    for (int ${1:i} = ${2:start}; ${1:i}>=0; ${1:i}--) {
        ${0}
    }

snippet statf
	static ${1:type} ${2:name}(${3:args}) {
		${0}
	}

snippet inlf
	inline ${1:type} ${2:name}(${3:args}) {
		${0}
	}

snippet arr
    ${1:type} ${2:name}[${3:amount}]${0}

snippet arrz
    ${1:type} ${2:name}[${3:amount}] = {0}${0}

snippet arrn
    ${1:name}[${2:amount}]${0}
