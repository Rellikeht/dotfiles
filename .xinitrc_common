#!/bin/sh
[ -e "~/.xinitrc_local" ] && ~/.xinitrc_local
xrdb .Xresources
keynav daemonize &
xcompmgr -fcD 1 &
unclutter --timeout 1.2 &
xrandr --output HDMI-0 --mode 2560x1440 --rate 75
nitrogen --restore
xmodmap ~/.xmodmap/qwerty
sleep 0.3
xmodmap ~/.xmodmaprc
xbindkeys
~/.audio_start.sh
playerctld daemon
nvidia-settings --load-config-only
