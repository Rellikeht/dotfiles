#!/bin/sh
[ -e "$HOME/.xinitrc_local" ] && ~/.xinitrc_local
xrdb .Xresources
keynav daemonize &
xcompmgr -fcD 1 &
unclutter --timeout 1.2 &
nitrogen --restore &
xbindkeys
~/.audio_start.sh &
playerctld daemon &
#xrandr --output HDMI-0 --mode 2560x1440 --rate 75 &
#nvidia-settings --load-config-only
sleep 4
setxkbmap -option caps:escape
xmodmap ~/.xmodmap/qwerty_start
