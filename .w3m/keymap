# Some keycodes:
# Ctrl      : C-, ^
# Escape    : ESC-, M-, ^[
# Space     : SPC, ' '
# Tab       : TAB, ^i, ^I
# Delete    : DEL, ^?
# Up	    : UP, ^[[A
# Down	    : DOWN, ^[[B
# Right	    : RIGHT, ^[[C
# Left	    : LEFT, ^[[D

# And tutorial
# http://w3m.rocks/howto/

# GOOD

keymap h LEFT
keymap j UP
keymap k DOWN
keymap l RIGHT

keymap < SHIFT_LEFT
keymap > SHIFT_RIGHT

keymap H MOVE_LEFT
keymap J MOVE_DOWN
keymap K MOVE_UP
keymap L MOVE_RIGHT

keymap gg BEGIN
keymap G END

keymap w NEXT_WORD
keymap b PREV_WORD

keymap UP MOVE_UP
keymap DOWN MOVE_DOWN
keymap RIGHT MOVE_RIGHT
keymap LEFT MOVE_LEFT

keymap 0 LINE_BEGIN
keymap $ LINE_END

keymap ? ISEARCH_BACK
keymap / ISEARCH
keymap n SEARCH_NEXT
keymap N SEARCH_PREV

# Probably not needed
#keymap / SEARCH
#keymap ? SEARCH_BACK

keymap C-z SUSPEND
keymap C-l REDRAW
keymap C-g LINE_INFO
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap C-d NEXT_HALF_PAGE
keymap C-u PREV_HALF_PAGE

# Works on moves
keymap u UNDO
keymap C-r REDO

keymap ! SHELL
keymap | PIPE_BUF
keymap # PIPE_SHELL

keymap ZZ EXIT
keymap ZQ ABORT

keymap :w SAVE
keymap :e EDIT
keymap :! EXEC_SHELL

keymap o GOTO

# Incompatible, but good

keymap O LOAD

keymap :W PRINT
keymap :E EDIT_SCREEN
keymap :g GOTO_LINE
keymap :G GOTO

keymap gh PREV_TAB
keymap gl NEXT_TAB
keymap R RELOAD
keymap t TAB_LINK
keymap T NEW_TAB
keymap Q CLOSE_TAB

keymap gj PREV
keymap gk NEXT
keymap gH TAB_LEFT
keymap gL TAB_RIGHT
keymap gw WRAP_TOGGLE

keymap { PREV_LINK
keymap } NEXT_LINK

keymap zh CURSOR_TOP
keymap zm CURSOR_MIDDLE
keymap zl CURSOR_BOTTOM

